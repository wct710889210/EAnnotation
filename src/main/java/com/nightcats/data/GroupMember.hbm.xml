<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nightcats.data.GroupMember" table="groupmember">
        <id name="id" column="id" type="int">
            <generator class="increment"/>
        </id>
        <property name="userId" column="user_id" type="java.lang.Integer" not-null="true"/>
        <property name="groupId" column="group_id" type="java.lang.Integer" not-null="true"/>
    </class>
</hibernate-mapping>